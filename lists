#include <iostream>
#include <cmath>
using namespace std;


int main(){
 float n [10];
  cout << "Enter an integer: " ;
  cin >> n[0];

  cout << "Enter an integer: ";
  cin >> n[1];

  cout << "Enter an integer: ";
  cin >> n[2];

  cout << "Enter an integer: ";
  cin >> n[3];

  cout << "Enter an integer: ";
  cin >> n[4];

  cout << "Enter an integer: " ;
  cin >> n[5];

  cout << "Enter an integer: ";
  cin >> n[6];

  cout << "Enter an integer: ";
  cin >> n[7];

  cout << "Enter an integer: ";
  cin >> n[8];

  cout << "Enter an integer: ";
  cin >> n[9];


float sum=0;
for(int x= 0; x<10; x++){
sum = sum + n[x];
}

float med= sum/10;

float stdd=0;
for(int x= 0; x<10; x++){
  stdd= stdd + ((n[x]- med)* (n[x]-med));
}
stdd= stdd/9;
stdd= sqrt(stdd);



cout << "The total sum is: " << sum << endl;
cout << "The average is: " << med  << endl;
cout << "The standard deviation is: " << stdd<<endl;

return 0;
}
